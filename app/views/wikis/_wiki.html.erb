<tr>
  <td><%= link_to wiki.title, wiki_path(wiki) %></td>
  <td><%= time_ago_in_words(wiki.created_at) %> ago by <%= link_to wiki.user.name, user_path(wiki.user) %></td>
  <% if @user.premium? || @user.admin? %>
    <td><%= link_to 'View Collaborators', wiki_collaborators_path(wiki) %></td>
    <td><%= link_to 'Add Collaborator', new_wiki_collaborator_path(wiki) %></td>
    <td><%= link_to 'Delete', wiki, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  <% elsif current_user %>
    <td><%= link_to "Edit", edit_wiki_path(wiki) %></td>
    <td><%= link_to 'Delete', wiki, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  <% end %>
</tr>