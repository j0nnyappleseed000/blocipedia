<div>
  <h1 class= "h3-text-devise"><%= @wiki.title %></h1>
  <%= @wiki.inspect %>
  <p><%= @wiki.body %></p>
   <div>
    <%= link_to "Edit", edit_wiki_path(@wiki), class: 'btn btn-info' %>
    <%= link_to "Delete", @wiki, method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete this wiki?' } %>
   </div>
</div>